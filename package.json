{
  "name": "xlsx-populate",
  "version": "1.20.0",
  "description": "Excel XLSX parser/generator written in JavaScript with Node.js and browser support, jQuery/d3-style method chaining, and a focus on keeping existing workbook features and styles in tact.",
  "keywords": [
    "excel",
    "workbook",
    "xlsx",
    "parse",
    "generate",
    "populate",
    "template",
    "browser",
    "style",
    "formatting",
    "encryption",
    "password"
  ],
  "main": "lib/XlsxPopulate.js",
  "browser": {
    "XlsxPopulate": "./browser/xlsx-populate.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jasmine-ts --config=test/unit/jasmine.json --project=test/tsconfig.json",
    "test:watch": "nodemon -e js,ts -w src -w test/unit -x npm test",
    "e2e-generate": "jasmine JASMINE_CONFIG_PATH=test/e2e-generate/jasmine.json",
    "e2e-parse": "jasmine JASMINE_CONFIG_PATH=test/e2e-parse/jasmine.json",
    "docs": "typedoc --mode file --out docs --excludePrivate --excludeNotExported src/index.ts",
    "docs:watch": "nodemon -e js,ts -w src -x npm run docs",
    "serve-docs": "http-server docs/ -c-1 -o",
    "lint": "tslint src/**/*.ts",
    "karma": "karma start --single-run",
    "karma:watch": "karma start",
    "blank": "ts-node -P blank/tsconfig.json blank/build.ts"
  },
  "author": "Dave T. Johnson <dave@dtjohnson.net>",
  "contributors": [
    "Eddie Corrigall <eddiecorrigall@gmail.com>",
    "Alan Doyle <alandoyle95@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "cfb": "^1.1.2",
    "jszip": "^3.2.2",
    "lodash": "^4.17.15",
    "sax": "^1.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dtjohnson/xlsx-populate.git"
  },
  "bugs": "https://github.com/dtjohnson/xlsx-populate/issues",
  "devDependencies": {
    "@types/jasmine": "^3.3.14",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.6.8",
    "@types/proxyquire": "^1.3.28",
    "@types/sax": "^1.2.0",
    "edge-js": "^12.3.1",
    "http-server": "^0.11.1",
    "jasmine": "^3.4.0",
    "jasmine-ts": "^0.3.0",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-typescript": "^4.1.1",
    "nodemon": "^1.19.1",
    "proxyquire": "^2.1.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  }
}
