{
  "name": "xlsx-populate",
  "version": "1.19.1",
  "description": "Excel XLSX parser/generator written in JavaScript with Node.js and browser support, jQuery/d3-style method chaining, and a focus on keeping existing workbook features and styles in tact.",
  "keywords": [
    "excel",
    "workbook",
    "xlsx",
    "parse",
    "generate",
    "populate",
    "template",
    "browser",
    "style",
    "formatting",
    "encryption",
    "password"
  ],
  "main": "lib/XlsxPopulate.js",
  "browser": {
    "XlsxPopulate": "./browser/xlsx-populate.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jasmine-ts --config=jasmine.json --project=tsconfig.spec.json",
    "test:watch": "nodemon -e js,ts -w src -x npm test",
    "e2e-generate": "jasmine JASMINE_CONFIG_PATH=test/e2e-generate/jasmine.json",
    "e2e-parse": "jasmine JASMINE_CONFIG_PATH=test/e2e-parse/jasmine.json",
    "docs": "typedoc --out docs --excludePrivate --excludeNotExported",
    "lint": "tslint src/**/*.ts",
    "karma": "karma start --single-run",
    "karma:watch": "karma start",
    "blank": "node blank/build"
  },
  "author": "Dave T. Johnson <dave@dtjohnson.net>",
  "contributors": [
    "Eddie Corrigall <eddiecorrigall@gmail.com>",
    "Alan Doyle <alandoyle95@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "cfb": "^1.1.0",
    "jszip": "^3.1.5",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.11",
    "sax": "^1.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dtjohnson/xlsx-populate.git"
  },
  "bugs": "https://github.com/dtjohnson/xlsx-populate/issues",
  "devDependencies": {
    "@types/jasmine": "^3.3.9",
    "@types/lodash-es": "^4.17.2",
    "@types/node": "^10.12.21",
    "@types/proxyquire": "^1.3.28",
    "@types/sax": "^1.0.1",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "edge-js": "^11.3.0",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.1",
    "jasmine": "^3.3.1",
    "jasmine-ts": "^0.3.0",
    "jsdoc-to-markdown": "^4.0.1",
    "jsondiffpatch": "^0.3.11",
    "karma": "^4.0.0",
    "karma-browserify": "^6.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^2.0.1",
    "karma-typescript": "^4.0.0",
    "markdown-include": "^0.4.3",
    "markdown-toc": "^1.2.0",
    "nodemon": "^1.18.10",
    "proxyquire": "^2.1.0",
    "proxyquire-universal": "^2.1.0",
    "proxyquireify": "^3.2.1",
    "replacestream": "^4.0.3",
    "run-sequence": "^2.2.1",
    "ts-node": "^8.0.2",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.13.0",
    "tslint-consistent-codestyle": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typescript": "^3.3.3",
    "uglify-js": "^3.4.9",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1"
  }
}
